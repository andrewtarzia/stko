integrator              = md       
nsteps                  = 100000    ; 200 ps
dt                      = 0.002     ; 2 fs
; Output control
nstxout                 = 0         
nstvout                 = 0         
nstfout                 = 0         
nstenergy               = 5000      
nstlog                  = 5000      
nstxout-compressed      = 5000      
; Bond parameters
continuation            = no        
constraint_algorithm    = lincs     
constraints             = h-bonds   
lincs_iter              = 1         
lincs_order             = 4         
; Nonbonded settings 
cutoff-scheme           = Verlet    
ns_type                 = grid      
nstlist                 = 10        
rcoulomb                = 1.0       
rvdw                    = 1.0       
DispCorr                = EnerPres  
; Electrostatics
coulombtype             = PME       
pme_order               = 4         
fourierspacing          = 0.16      
; Temperature coupling is on
tcoupl                  = v-rescale 
tc-grps                 = System   
tau_t                   = 0.1       
ref_t                   = 300       
; Pressure coupling is off
pcoupl                  = no        
; Periodic boundary conditions
pbc                     = xyz       
; Velocity generation
gen_vel                 = yes       
gen_temp                = 300       
gen_seed                = -1        

